            android:name="com.sixtech.paulchidi.riddleme.game.GameActivity"
            android:label="@string/title_activity_game"
            android:parentActivityName="com.sixtech.paulchidi.riddleme.MainActivity" />
        <activity
            android:name="com.sixtech.paulchidi.riddleme.HelpActivity"
            android:label="@string/title_activity_help" />
